<h1>New transaction</h1>

<% form_for(@transaction,:html => {:multipart => true}) do |f| %>
  <%= f.error_messages %>
    
  <p>
    <%= label_tag "file", "File to upload" %>
    <%= f.file_field("file") %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', transactions_path %>
